<div class="navigation_mobile" data-widget="default_mobile_navigation">
  <div class="menu index"></div>
  <div class="menu sharing"></div>
  <div class="menu imprint"></div>
  <div class="overview_mobile">
      <span class="heading-1"><%= t('.overview') %></span>
      <div class="wrapper">
        <ul>
          <%= render :partial => 'pageflow/entries/navigation/page',
                     :collection => entry_mobile_navigation_pages(entry),
                     :locals => {:image_style => :thumbnail_overview_mobile, :image_width => nil} %>
        </ul>
      </div>
  </div>
  <div class="imprint_mobile">
      <span class="heading-1"><%= t('.imprint') %></span>
      <div class="wrapper">
        <ul>
          <li class="credits">
            <%= render 'pageflow/entries/navigation/credits_box_content', :entry =>  entry %>
          </li>
        </ul>
      </div>
  </div>
</div>
